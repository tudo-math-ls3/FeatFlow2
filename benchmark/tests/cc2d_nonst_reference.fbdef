testid   = CC2D_NONST1REF4_INIT_6
descr    = standard DFG benchmark 1, CC2D, nonstationary Navier-Stokes, Q2, SD(0.0), Reference 4
mglevels = 6
DISCRETISATION_SINITIALSOLUTIONFILENAME = './logs/CC2D_NONST1REF3_INIT_6/solution'

