# Position of the constant coefficient matrix S in the scalar matrix structure
CoeffMatrix_CX = 6

# Position of the constant coefficient matrix S in the scalar matrix structure
CoeffMatrix_CY = 7

# Position of the constant coefficient matrix S in the scalar matrix structure
CoeffMatrix_CZ = 8

