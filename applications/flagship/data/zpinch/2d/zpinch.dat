celement = %{Hydro.celement}

# convert mesh to triangular mesh?
iconvToTria = %{Hydro.iconvToTria}

# type of matrix format
# 7 = matrix format 7
# 9 = matrix format 9
imatrixformat = %{Hydro.imatrixformat}

# type of Jacobian matrix
# 0 = standard sparsity pattern
